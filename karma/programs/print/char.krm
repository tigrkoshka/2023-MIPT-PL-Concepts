.__newline_char: char '\n'

# does not accept arguments, prints a newline character
print_newline:
  # r0 = '\n'
  load r0 .__newline_char

  # print the newline character
  syscall r0 105

  # return
  ret 0

# accepts a single argument: the character to be printed
print_char:
    # load the character to be printed to r0
    loadr r0 r14 3

    # print the character
    syscall r0 105

    # return
    ret 0
